{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-underscore-dangle": "off",
        "no-undef": "off",
        "no-shadow": "off",
        "no-use-before-define": "off",
        "class-methods-use-this": "off",
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-param-reassign": "off",
        "no-unused-vars": "off",
        "no-await-in-loop": "off"
    }
}